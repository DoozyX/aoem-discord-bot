{
  "name": "aoem-discord-bot",
  "version": "1.0.0",
  "author": "DoozyX",
  "description": "A discord.js bot template written with TypeScript",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": [
    "./src/start-bot.ts",
    "./src/start-manager.js"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "git clean -xdf --exclude=\"/config/**/*\"",
    "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
    "build": "tsc --project tsconfig.json",
    "commands:view": "ts-node -r tsconfig-paths/register src/start-bot.ts commands view",
    "commands:register": "ts-node -r tsconfig-paths/register src/start-bot.ts commands register",
    "commands:rename": "ts-node -r tsconfig-paths/register src/start-bot.ts commands rename",
    "commands:delete": "ts-node -r tsconfig-paths/register src/start-bot.ts commands delete",
    "commands:clear": "ts-node -r tsconfig-paths/register src/start-bot.ts commands clear",
    "start": "yarn run start:bot",
    "start:bot": "ts-node -r tsconfig-paths/register src/start-bot.ts",
    "start:manager": "ts-node src/start-manager.ts",
    "dev:bot": "ts-node-dev -r tsconfig-paths/register src/start-bot.ts",
    "start:pm2": "yarn run pm2:start",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:delete": "pm2 delete process.json"
  },
  "dependencies": {
    "@discordjs/rest": "2.4.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cron-parser": "^4.9.0",
    "discord.js": "14.17.3",
    "discord.js-rate-limiter": "1.3.2",
    "express": "4.21.2",
    "express-promise-router": "4.1.1",
    "filesize": "10.1.6",
    "linguini": "1.3.1",
    "luxon": "3.5.0",
    "node-fetch": "2.7.0",
    "node-schedule": "2.1.1",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "pm2": "^5.4.3",
    "reflect-metadata": "^0.2.2",
    "remove-markdown": "0.6.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/express": "5.0.0",
    "@types/luxon": "3.4.2",
    "@types/node": "^22.13.0",
    "@types/node-schedule": "2.1.7",
    "@types/remove-markdown": "0.3.4",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "typescript-transform-paths": "^3.5.3"
  }
}
